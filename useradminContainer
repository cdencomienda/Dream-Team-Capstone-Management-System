<!-- <!DOCTYPE html>
<html lang="en">
<head>
<meta charset="UTF-8">
<meta name="viewport" content="width=device-width, initial-scale=1.0">
<title>User Data</title>
<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
</head>
<body>

<h2>User Data</h2>
<div id="user-data"></div>

<script>
$(document).ready(function(){
    // AJAX request to fetch user data
    $.ajax({
        url: 'get_users.php',
        type: 'GET',
        success: function(response){
            // Parse JSON response
            var users = JSON.parse(response);
            // Create HTML for displaying user data
            var userDataHTML = '<table>';
            userDataHTML += '<tr><th>User ID</th><th>User Type</th><th>User Name</th><th>User Email</th></tr>';
            users.forEach(function(user){
                userDataHTML += '<tr>';
                userDataHTML += '<td>' + user.userID + '</td>';
                userDataHTML += '<td>' + user.userType + '</td>';
                userDataHTML += '<td>' + user.userName + '</td>';
                userDataHTML += '<td>' + user.userEmail + '</td>';
                userDataHTML += '</tr>';
            });
            userDataHTML += '</table>';
            // Display user data on the page
            $('#user-data').html(userDataHTML);
        }
    });
});
</script>

</body>
</html> -->
